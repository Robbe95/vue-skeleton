    "@headlessui/vue": "^1.7.16",
    "@intlify/unplugin-vue-i18n": "^1.4.0",
    "@tanstack/vue-query": "^4.35.7",
    "@tanstack/vue-table": "^8.10.3",
    "@vueuse/core": "^10.4.1",
    "@vueuse/head": "^2.0.0",
    "axios": "^1.5.1",
    "cva": "1.0.0-beta.1",
    "formango": "^1.0.3",
    "pinia": "^2.1.6",
    "sass": "^1.68.0",
    "tailwind-merge": "^1.14.0",
    "vue": "^3.3.4",
    "vue-i18n": "^9.5.0",
    "vue-router": "^4.2.5",
    "zod": "^3.22.2"
